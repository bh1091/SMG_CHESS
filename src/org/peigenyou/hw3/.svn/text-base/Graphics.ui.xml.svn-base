<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
   ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator">
  <ui:style field="css" type="org.peigenyou.hw3.GameCss">
    .highlighted {
      outline: solid red 3px;
    }
    .dotted{
    outline: dotted green 1px;
  }
  .fontSize{
    font-size:18px;
    color:red;
  }
  .myBoard{ 
        background-image: url('http://digital-static.net/Files/Source/Chess/Classes/Graphics/ChessBoard.png'); 
  } 
    td img {
      display: block
    }
  </ui:style>
  <g:HTMLPanel>
    <g:Label ui:field="userInfo" text=""/>
     
    <g:AbsolutePanel ui:field="container"/>
    <g:Grid ui:field="controlPanel"/>
    <g:Label ui:field="matchInfo" text=""/>
    <g:Label ui:field="gameStatus" text=""/>
    
  </g:HTMLPanel>
</ui:UiBinder> 