package org.peigenyou.hw3;

import com.google.gwt.user.client.rpc.RemoteService;
import com.google.gwt.user.client.rpc.RemoteServiceRelativePath;

/**
 * The client side stub for the RPC service.
 */
@RemoteServiceRelativePath("service")
public interface GreetingService extends RemoteService {
  Player greetServer(String name);
  String autoMatch(String player,String newState);
  void submitMove(String name,String state);
  Match loadMatch(String ID);
  void creatMatch(Match match);
  void delMatch(String ID);
  Player updatePlayer();
  void sendResult(Match match,String result);
  
}
