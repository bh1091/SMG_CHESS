<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget">

  <g:HTMLPanel>
    <div style="margin:2em" align="center" padding-left="5em">
      <g:HorizontalPanel spacing="10">
        <g:SuggestBox ui:field="emailBox" text="" />
        <g:Button ui:field="emailButton">
          New Match
        </g:Button>
      </g:HorizontalPanel>
      <g:HorizontalPanel spacing="10">
        <g:Button ui:field="autoButton">
          Auto Match
        </g:Button>
        <g:Button ui:field="deleteButton">
          Delete Current Match
        </g:Button>
      </g:HorizontalPanel>
      <g:HorizontalPanel spacing="10">
        <g:Button ui:field="aiButton">
          Play against the computer
        </g:Button>
      </g:HorizontalPanel>
      <g:HorizontalPanel spacing="10">
        <g:Button ui:field="closeButton">
          Return to Game
        </g:Button>
      </g:HorizontalPanel>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>