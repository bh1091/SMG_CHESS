package org.zhaohuizhang.hw7;
import com.googlecode.objectify.Key;
import com.googlecode.objectify.annotation.Entity;
import com.googlecode.objectify.annotation.Id;

@Entity
public class Match {
	@Id
	Long matchid;
	Key<Player> black;
	Key<Player> white;
	String state;
	boolean isOver;
	
	public Match(){}
	
	public Match(Key<Player> white, Key<Player> black, String state, boolean isOver){
		this.black = black;
		this.white = white;
		this.state = state;
		this.isOver = isOver;
	}

	public Key<Player> getBlack() {
		return black;
	}

	public void setBlack(Key<Player> black) {
		this.black = black;
	}

	public Key<Player> getWhite() {
		return white;
	}

	public void setWhite(Key<Player> white) {
		this.white = white;
	}

	public String getState() {
		return state;
	}

	public void setState(String state) {
		this.state = state;
	}

	public boolean isOver() {
		return isOver;
	}

	public void setOver(boolean isOver) {
		this.isOver = isOver;
	}
	
	
}

