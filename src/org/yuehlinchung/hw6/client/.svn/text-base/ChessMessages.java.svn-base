package org.yuehlinchung.hw6.client;

import com.google.gwt.i18n.client.Messages;

public interface ChessMessages  extends Messages {
	@DefaultMessage("Rank: {0}")
	String rank(int rank);
	@DefaultMessage("English")
	String english();
	@DefaultMessage("Chinese")
	String chinese();
	@DefaultMessage("Email: {0}")
	String email(String email);
	@DefaultMessage("Player: {0}")
	String name(String name);
	@DefaultMessage("Please sign in to your Google Account to access the ChessGame application.")
	String autoSignIn();
	@DefaultMessage("Sign In")
	String signIn();
	@DefaultMessage("Sign Out")
	String signOut();
	@DefaultMessage("Auto Matching Game")
	String autoMatchingGame();
	@DefaultMessage("Cancel Matching Game")
	String cancelMatchingGame();
	@DefaultMessage("Delete")
	String delete();
	@DefaultMessage("Start a match/game")
	String startMatchGame();
	@DefaultMessage("Play against the computer")
	String playWithAI();
	@DefaultMessage("Friend''s Matching:")
	String friendsMatching();
	@DefaultMessage("Looking for a game...")
	String lookinForGame();
	@DefaultMessage("or")
	String or();
