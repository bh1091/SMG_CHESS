package org.bohouli.hw6.client;

import java.util.Date;

import com.google.gwt.i18n.client.Messages;

public interface BohouChessMessages extends Messages {
	@DefaultMessage("Match Begin Time: {0,date,medium} {0,time,medium}")
	String matchTime(Date timestamp);
	@DefaultMessage("User Name: ''{0}''")
	String userName(String userName);
	@DefaultMessage("Email Address: ''{0}''")
	String emailAddress(String email);
	@DefaultMessage("Opponent Name: ''{0}''")
	String opponentName(String email);
	@DefaultMessage("Rank: ''{0}''")
	String userRank(String rank);
}
