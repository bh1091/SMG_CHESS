package org.markanderson.hw3;

import com.google.gwt.resources.client.ClientBundle;
import com.google.gwt.resources.client.ImageResource;

public interface GameImages extends ClientBundle {
	@Source("chessbaord.png")
	ImageResource chessboard();

	// blank piece
	@Source("blank.png")
	ImageResource blank();

	// black pieces
	@Source("black_bishop.png")
	ImageResource bBishop();

	@Source("black_king.png")
	ImageResource bKing();

	@Source("black_knight.png")
	ImageResource bKnight();

	@Source("black_pawn.png")
	ImageResource bPawn();

	@Source("black_queen.png")
	ImageResource bQueen();

	@Source("black_rook.png")
	ImageResource bRook();

	// white pieces
	@Source("white_bishop.png")
	ImageResource wBishop();

	@Source("white_king.png")
	ImageResource wKing();

	@Source("white_knight.png")
	ImageResource wKnight();

	@Source("white_pawn.png")
	ImageResource wPawn();

	@Source("white_queen.png")
	ImageResource wQueen();

	@Source("white_rook.png")
	ImageResource wRook();
}
